        "-Wno-inconsistent-missing-override",
        "-Wno-unused-parameter",
        "-Wno-unused-function",
    ],
    static_libs: [
        "gfxstream_base",
    ],
    srcs: [
        "etc.cpp",
        "rgtc.cpp",
        "FramebufferData.cpp",
        "GLBackgroundLoader.cpp",
        "GLDispatch.cpp",
        "GLESbuffer.cpp",
        "GLEScontext.cpp",
        "GLESpointer.cpp",
        "GLESvalidate.cpp",
        "GLutils.cpp",
        "NamedObject.cpp",
        "ObjectData.cpp",
        "ObjectNameSpace.cpp",
        "PaletteTexture.cpp",
        "RangeManip.cpp",
        "SaveableTexture.cpp",
        "ScopedGLState.cpp",
        "ShareGroup.cpp",
        "TextureData.cpp",
        "TextureUtils.cpp",
    ],
}
